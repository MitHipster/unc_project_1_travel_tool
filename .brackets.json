{
    "fonts.fontSize": "15px",
    "fonts.fontFamily": "'SourceCodePro-Medium', ＭＳ ゴシック, 'MS Gothic', monospace",
    "useTabChar": false,
    "spaceUnits": 2,
    "themes.theme": "dark-theme",
    "styleActiveLine": true,
    "livedev.multibrowser": true,
    "highlightMatches": true,
    "linting.collapsed": false,
    "language": {
        "javascript": {
            "linting.prefer": "JSHint",
            "linting.usePreferredOnly": true
        }
    },
    "mikaeljorhult.bracketsAutoprefixer.visualCascade": false,
    "mikaeljorhult.bracketsAutoprefixer.browsers": [
        "> 1%",
        "last 2 versions",
        "Firefox ESR"
    ]
}